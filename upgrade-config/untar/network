
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option ifname 'lan0 wan0'
	option type 'bridge'
	option proto 'dhcp'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'

config interface 'wan'
	option proto 'dhcp'
	option ipaddr '10.0.0.1'
	option netmask '255.0.0.0'
	option enabled '0'

config interface 'mgt'
	option type 'bridge'
	option ifname ' '
	option proto 'dhcp'
	option enabled '0'
	option metric '50'

config interface 'vlp1'
	option enabled '0'
	option type 'bridge'
	option proto 'static'
	option ipaddr '0.0.0.0'
	option ports ' '
	option ifname ' '
	option tagged '0'

config interface 'vlp2'
	option enabled '0'
	option type 'bridge'
	option proto 'static'
	option ipaddr '0.0.0.0'
	option ports ' '
	option ifname ' '
	option tagged '0'

config interface 'vlp3'
	option enabled '0'
	option type 'bridge'
	option proto 'static'
	option ipaddr '0.0.0.0'
	option ports ' '
	option ifname ' '
	option tagged '0'

