
config wifi-device 'radio0'
	option type 'mac80211'
	option disabled '0'
	option channel 'auto'
	option hwmode '11g'
	option htmode 'HT40'
	option txpower '20'
	option distance '50'
	option antenna_gain '4'
	option path 'platform/qca953x_wmac'
	option country 'DE'

config wifi-iface 'wlan0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option wds '1'
	option bgscan '1'
	option ssid 'airCube-B4FBE4622C2D'
	option encryption 'psk2'
	option key 'DA6RJBrNxws'

config wifi-device 'radio1'
	option type 'mac80211'
	option path 'pci0000:00/0000:00:00.0'
	option disabled '0'
	option channel 'auto'
	option hwmode '11a'
	option htmode 'VHT80'
	option txpower '20'
	option distance '50'
	option antenna_gain '5'
	option country 'DE'

config wifi-iface 'wlan1'
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option wds '1'
	option bgscan '1'
	option ssid 'airCube-B4FBE4622C2D'
	option encryption 'psk2'
	option key 'DA6RJBrNxws'

